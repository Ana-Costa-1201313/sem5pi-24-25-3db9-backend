@startuml
autonumber

participant "<<Admin>>\nAdmin" as admin
participant "BE" as be

admin -> be: POST /patients
activate admin
activate be
    alt patient profile not created
        [<-- be : 422: Unprocessable Content
    else patient profile created successfully
        [<-- be : 201: Created
    end
deactivate be
deactivate admin
@enduml
