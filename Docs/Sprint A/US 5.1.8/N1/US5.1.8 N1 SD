@startuml US8_nivel1
autonumber

participant "<<Admin>>\nAdmin" as admin
participant "<<system>>\nSystem HealthcareApp" as sys
activate admin
admin -> sys: POST /patients

activate sys
    alt patient profile not created
        [<-- sys : 422: Unprocessable Content
    else patient profile created successfully
        [<-- sys : 201: Created
    end
deactivate sys
deactivate admin
@enduml